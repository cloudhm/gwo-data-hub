# 数据库配置
DATABASE_URL="postgresql://username:password@localhost:5432/gwodatahub?schema=public"

# 服务器配置
PORT=3000
NODE_ENV=development

# 亚马逊SP-API配置
# 从 Amazon Seller Central 获取这些凭证
AMAZON_CLIENT_ID=your_amazon_client_id
AMAZON_CLIENT_SECRET=your_amazon_client_secret

# AWS 凭证（用于 SP-API 签名）
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_SELLING_PARTNER_ROLE=arn:aws:iam::123456789012:role/SellingPartnerRole

# 领星ERP API配置
# API域名: https://openapi.lingxing.com
# 从领星ERP后台获取APP ID和APP Secret
LINGXING_APP_KEY=your_lingxing_app_key
LINGXING_APP_SECRET=your_lingxing_app_secret

